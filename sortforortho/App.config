<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<!--Sökväg till rootmappen för alla bilder-->
		<add key="pathToFiles" value="C:\Users\linus\LNU\2dv50e\images"/>

		<!--Anger om sökning av bilder även ska ske i undermappar, ange true för rekursiv sökning, false för endast rootmapp.-->
		<add key="searchRecursive" value="true"/>

		<!--Anger vilka filtyper som ska hanteras av programmet (exempel: "jpg,jpeg"-->
		<add key="filter" value="jpg,jpeg"/>
	</appSettings>
</configuration>