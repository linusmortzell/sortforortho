<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<!--Sökväg till rootmappen för alla bilder-->
		<add key="pathToFiles" value="C:\Users\linus\LNU\2dv50e\images"/>

		<!--Sökväg till katalog vars färdigsorterade batcher ska placeras, varje batch hamnar i en egen katalog i den katalogen-->
		<add key="pathToSortedBatches" value ="./Batches"/>

		<!--Sensorvidden på kameran i drönaren, ange storleken i millimeter, exempel: M2P "13,2" M2 "6,3"-->
		<add key="sensorWidth" value="13,2"/>

		<!--Värde i procent över hur mycket överlapp bilder ska ha för att tillhöra samma ortofoto-->
		<add key="overlapPercentage" value="50"/>

		<!--Anger om sökning av bilder även ska ske i undermappar, ange true för rekursiv sökning, false för endast rootmapp.-->
		<add key="searchRecursive" value="true"/>

		<!--Anger max antal sekunder som är tillåtet mellan bilder inom en och samma batch, anges i hela sekunder-->
		<add key="maxSecondsBetweenImages" value="7200"/>

		<!--Anger vilka filtyper som ska hanteras av programmet (exempel: "jpg,jpeg"-->
		<add key="filter" value="jpg,jpeg"/>

		<!--Anger url till Node ODM API:et-->
		<add key="nodeOdmUrl" value="http://localhost:59053"/>

		<add key="username" value="linus"/>
		<add key="password" value="Reflex21"/>
	</appSettings>
</configuration>